<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="nt:unstructured">
    <button jcr:primaryType="nt:unstructured"
        resourceTypePath="dexter/components/content/cta">
        <spectrumLink
            jcr:primaryType="nt:unstructured"
            text="Spectrum Link - Default"
            value="spectrum-Link"/>
        <spectrumCta
            jcr:primaryType="nt:unstructured"
            text="Spectrum Button - CTA"
            value="spectrum-Button spectrum-Button--cta"/>
        <spectrumOverBackground
            jcr:primaryType="nt:unstructured"
            text="Spectrum Button - Over Background"
            value="spectrum-Button spectrum-Button--overBackground"/>
        <spectrumPrimary
            jcr:primaryType="nt:unstructured"
            text="Spectrum Button - Primary"
            value="spectrum-Button spectrum-Button--primary"/>
        <spectrumLinkQuiet
            jcr:primaryType="nt:unstructured"
            text="Spectrum Link - Quiet"
            value="spectrum-Link spectrum-Link--quiet"/>
        <spectrumLinkOverBackground
            jcr:primaryType="nt:unstructured"
            text="Spectrum Link - Over Background"
            value="spectrum-Link spectrum-Link--overBackground"/>
    </button>
    <languageNavigation jcr:primaryType="nt:unstructured"
        resourceTypePath="dexter/components/content/languageNavigation" />
    <subnavLink jcr:primaryType="nt:unstructured"
        resourceTypePath="dexter/components/restricted/subnavLink" />
    <table jcr:primaryType="nt:unstructured"
        resourceTypePath="dexter/components/structure/table" />
    <structurePartials jcr:primaryType="nt:unstructured"
        resourceTypePath="dexter/components/structure/partials" />
    <page jcr:primaryType="nt:unstructured">
        <spectrumLightest
            jcr:primaryType="nt:unstructured"
            text="Spectrum Lightest"
            value="spectrum spectrum--lightest"/>
        <spectrumLight
            jcr:primaryType="nt:unstructured"
            text="Spectrum Light"
            value="spectrum spectrum--light"/>
        <spectrumDark
            jcr:primaryType="nt:unstructured"
            text="Spectrum Dark"
            value="spectrum spectrum--dark"/>
        <spectrumDarkest
            jcr:primaryType="nt:unstructured"
            text="Spectrum Darkest"
            value="spectrum spectrum--darkest"/>
    </page>
    <position jcr:primaryType="nt:unstructured">
        <borderStyle jcr:primaryType="nt:unstructured">
            <none
                jcr:primaryType="nt:unstructured"
                text="None"
                value=""/>
            <solid
                jcr:primaryType="nt:unstructured"
                text="Solid"
                value="solid"/>
            <dotted
                jcr:primaryType="nt:unstructured"
                text="Dotted"
                value="dotted"/>
            <dashed
                jcr:primaryType="nt:unstructured"
                text="Dashed"
                value="dashed"/>
            <inset
                jcr:primaryType="nt:unstructured"
                text="Inset"
                value="inset"/>
            <outset
                jcr:primaryType="nt:unstructured"
                text="Outset"
                value="outset"/>
        </borderStyle>
    </position>
    <flexContainer jcr:primaryType="nt:unstructured">
        <widths jcr:primaryType="nt:unstructured">
            <auto
                jcr:primaryType="nt:unstructured"
                text="Auto"
                value="auto"/>
            <custom
                jcr:primaryType="nt:unstructured"
                text="Custom"
                value="custom"/>
            <fullWidth
                jcr:primaryType="nt:unstructured"
                text="Full Width"
                value="100%"/>
            <oneColumn
                jcr:primaryType="nt:unstructured"
                text="1 Column"
                value="8.333333%"/>
            <twoColumn
                jcr:primaryType="nt:unstructured"
                text="2 Columns"
                value="16.666667%"/>
            <threeColumn
                jcr:primaryType="nt:unstructured"
                text="3 Columns"
                value="25%"/>
            <fourColumn
                jcr:primaryType="nt:unstructured"
                text="4 Columns"
                value="33.333333%"/>
            <fiveColumn
                jcr:primaryType="nt:unstructured"
                text="5 Columns"
                value="41.666667%"/>
            <sixColumn
                jcr:primaryType="nt:unstructured"
                text="6 Columns"
                value="50%"/>
            <sevenColumn
                jcr:primaryType="nt:unstructured"
                text="7 Columns"
                value="58.333333%"/>
            <eightColumn
                jcr:primaryType="nt:unstructured"
                text="8 Columns"
                value="66.666667%"/>
            <nineColumn
                jcr:primaryType="nt:unstructured"
                text="9 Columns"
                value="75%"/>
            <tenColumn
                jcr:primaryType="nt:unstructured"
                text="10 Columns"
                value="83.333333%"/>
            <elevenColumn
                jcr:primaryType="nt:unstructured"
                text="11 Columns"
                value="91.666667%"/>
        </widths>
        <offsets jcr:primaryType="nt:unstructured">
            <none
                jcr:primaryType="nt:unstructured"
                text="None"
                value=""/>
            <oneColumn
                jcr:primaryType="nt:unstructured"
                text="1 Column"
                value="8.333333%"/>
            <twoColumn
                jcr:primaryType="nt:unstructured"
                text="2 Columns"
                value="16.666667%"/>
            <threeColumn
                jcr:primaryType="nt:unstructured"
                text="3 Columns"
                value="25%"/>
            <fourColumn
                jcr:primaryType="nt:unstructured"
                text="4 Columns"
                value="33.333333%"/>
            <fiveColumn
                jcr:primaryType="nt:unstructured"
                text="5 Columns"
                value="41.666667%"/>
            <sixColumn
                jcr:primaryType="nt:unstructured"
                text="6 Columns"
                value="50%"/>
            <sevenColumn
                jcr:primaryType="nt:unstructured"
                text="7 Columns"
                value="58.333333%"/>
            <eightColumn
                jcr:primaryType="nt:unstructured"
                text="8 Columns"
                value="66.666667%"/>
            <nineColumn
                jcr:primaryType="nt:unstructured"
                text="9 Columns"
                value="75%"/>
            <tenColumn
                jcr:primaryType="nt:unstructured"
                text="10 Columns"
                value="83.333333%"/>
            <elevenColumn
                jcr:primaryType="nt:unstructured"
                text="11 Columns"
                value="91.666667%"/>
        </offsets>
        <height jcr:primaryType="nt:unstructured">
            <auto
                jcr:primaryType="nt:unstructured"
                text="Auto"
                value="auto"/>
            <custom
                jcr:primaryType="nt:unstructured"
                text="Custom"
                value="custom"/>
        </height>
        <justification jcr:primaryType="nt:unstructured">
            <stretch
                jcr:primaryType="nt:unstructured"
                text="Stretch"
                value="JustifyStretch"/>
            <start
                jcr:primaryType="nt:unstructured"
                text="Start"
                value="JustifyStart"/>
            <center
                jcr:primaryType="nt:unstructured"
                text="Center"
                value="JustifyCenter"/>
            <end
                jcr:primaryType="nt:unstructured"
                text="End"
                value="JustifyEnd"/>
            <spaceBetween
                jcr:primaryType="nt:unstructured"
                text="Space Between"
                value="JustifySpaceBetween"/>
        </justification>
        <itemAlignment jcr:primaryType="nt:unstructured">
            <stretch
                jcr:primaryType="nt:unstructured"
                text="Stretch"
                value="AlignAlignmentStretch"/>
            <start
                jcr:primaryType="nt:unstructured"
                text="Start"
                value="AlignItemStart"/>
            <center
                jcr:primaryType="nt:unstructured"
                text="Center"
                value="AlignItemCenter"/>
            <end
                jcr:primaryType="nt:unstructured"
                text="End"
                value="AlignItemEnd"/>
        </itemAlignment>
        <contentAlignment
            jcr:primaryType="nt:unstructured">
            <stretch
                jcr:primaryType="nt:unstructured"
                text="Stretch"
                value="AlignContentStretch"/>
            <start
                jcr:primaryType="nt:unstructured"
                text="Start"
                value="AlignContentStart"/>
            <center
                jcr:primaryType="nt:unstructured"
                text="Center"
                value="AlignContentCenter"/>
            <end
                jcr:primaryType="nt:unstructured"
                text="End"
                value="AlignContentEnd"/>
            <spaceBetween
                jcr:primaryType="nt:unstructured"
                text="Space Between"
                value="AlignContentSpaceBetween"/>
        </contentAlignment>
        <itemContentAlignment jcr:primaryType="nt:unstructured">
            <start
                jcr:primaryType="nt:unstructured"
                text="Start"
                value="AlignItemContentStart"/>
            <center
                jcr:primaryType="nt:unstructured"
                text="Center"
                value="AlignItemContentCenter"/>
            <end
                jcr:primaryType="nt:unstructured"
                text="End"
                value="AlignItemContentEnd"/>
        </itemContentAlignment>
    </flexContainer>
    <faasform jcr:primaryType="nt:unstructured">
        <formBackgroundColor jcr:primaryType="nt:unstructured" sling:configCollectionInherit="{Boolean}true">
            <standard
                jcr:primaryType="nt:unstructured"
                text="White (default)"
                value=""/>
            <static
                jcr:primaryType="nt:unstructured"
                text="Dark"
                value="theme-dark"/>
        </formBackgroundColor>
        <formLayout jcr:primaryType="nt:unstructured" sling:configCollectionInherit="{Boolean}true">
            <singleColumn
                jcr:primaryType="nt:unstructured"
                text="1 Column (default)"
                value=""/>
            <twoColumn
                jcr:primaryType="nt:unstructured"
                text="2 Columns"
                value="theme-2cols"/>
        </formLayout>
        <customTheme jcr:primaryType="nt:unstructured" sling:configCollectionInherit="{Boolean}true">
            <none
                jcr:primaryType="nt:unstructured"
                text="None (default)"
                value=""/>
            <amc
                jcr:primaryType="nt:unstructured"
                text="AMC Theme"
                value="theme-amc"/>
        </customTheme>
    </faasform>
</jcr:root>
